            <geo:monumentDescription codeSpace="urn:ga-gov-au:monument-description-type">SCIGN SHORT DRILL-BRACED STAINLESS STEEL</geo:monumentDescription>
            <geo:heightOfTheMonument>1.7</geo:heightOfTheMonument>
            <geo:monumentFoundation>BASALT BEDROCK</geo:monumentFoundation>
